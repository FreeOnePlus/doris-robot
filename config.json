{
  "collection_name": "doris_docs",
  "log_level": "INFO",
  "timeout": 60,
  "max_question_length": 500,
  "sensitivity_check": true,
  "wechat": {
    "enable": true,
    "trigger_prefix": "",
    "admin_users": ["user1"],
    "rate_limit": 5
  },
  "model_config": {
    "services": {
      "chat": {
        "provider": "deepseek",
        "model": "deepseek-chat"
      },
      "generation": {
        "provider": "siliconflow",
        "model": "deepseek-ai/DeepSeek-V3"
      },
      "embedding": {
        "provider": "siliconflow",
        "model": "Pro/BAAI/bge-m3"
      },
      "moderation": {
        "provider": "deepseek",
        "model": "deepseek-chat"
      }
    },
    "providers": {
      "deepseek": {
        "api_key": "",
        "endpoint": "https://api.deepseek.com/v1"
      },
      "siliconflow": {
        "api_key": "",
        "endpoint": "https://api.siliconflow.cn/v1"
      }
    }
  },
  "milvus": {
    "host": "milvus_host",
    "port": 19530
  },
  "docs_path": "docs_path"
} 
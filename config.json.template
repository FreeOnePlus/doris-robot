{
  "collection_name": "doris_docs",
  "timeout": 60,
  "max_question_length": 500,
  "sensitivity_check": true,
  "wechat": {
    "enable": true,
    "trigger_prefix": "",
    "admin_users": ["user1"],
    "rate_limit": 5
  },
  "model_config": {
    "services": {
      "chat": {
        "provider": "deepseek",
        "model": "deepseek-chat",
        "temperature": 0.2
      },
      "generation": {
        "provider": "siliconflow",
        "model": "deepseek-ai/DeepSeek-V3",
        "temperature": 0.2
      },
      "embedding": {
        "provider": "siliconflow",
        "model": "Pro/BAAI/bge-m3",
        "temperature": 0.2
      },
      "moderation": {
        "provider": "deepseek",
        "model": "deepseek-chat",
        "temperature": 0.2,
        "max_tokens": 800
      }
    },
    "providers": {
      "deepseek": {
        "api_key": "sk-xxx",
        "endpoint": "https://api.deepseek.com/v1"
      },
      "siliconflow": {
        "api_key": "sk-yyy",
        "endpoint": "https://api.siliconflow.cn/v1"
      }
    }
  },
  "milvus": {
    "host": "localhost",
    "port": 19530
  },
  "docs_path": "docs_path",
  "data_paths": {
    "doris_docs": "./doris_docs",
    "jira_data": "./jira_data"
  },
  "search_config": {
    "min_similarity": 0.65,
    "version_weights": {
      "3.0": 1.5,
      "2.1": 1.3,
      "2.0": 1.0
    }
  },
  "moderation": {
    "enable_keyword": true,
    "keywords": ["doris", "数据库", "apache"],
    "fallback_strategy": "allow"
  },
  "vector_dimension": 1024,
  "logging_config": {
    "level": "INFO",
    "qa_debug": true
  }
} 